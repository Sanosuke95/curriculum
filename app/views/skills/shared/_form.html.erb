<%= form_with model: skill do |f| %>
    <div class="row">
        <div class="col">
            <%= f.label :title, "title", class: "form-label" %>
            <%= f.text_field :title, class: "form-control" %>
        </div>

        <div class="col">
            <%= f.label :title, "level", class: "form-label" %>
            <%= f.select :level, options_for_select(0..5), {},class: "form-select" %>
        </div>
    </div>

    <div></div>
    
    <div class="text-center">
        <%= f.submit 'Submit', class: "btn btn-primary" %>
    </div>

    <div class="d-grid gap-2 d-md-flex justify-content-md-end">
        <%= link_to "Back", :back, class: 'btn btn-secondary', type: "button" %>
        <%= link_to  "Pass", new_experience_path, class: 'btn btn-success', type: "button" %>
        </div>
<% end %>