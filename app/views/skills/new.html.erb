<h1>Skills</h1>

<h3>Title skill</h3>

<% if @skill_save %>
    <div class="container text-center">
        <div class="row">
            <% @skill_save.each do |skill| %>
                <div class="col-md-4">
                    <p><%= skill.title %> - <%= skill.level %></p>
                </div>
            <% end %>
        </div>
    </div>
<% end %>

<div data-controller="hello">
    <p data-action="hello#test"></p>
</div>

<div data-controller="hello">
  <input data-hello-target="name" type="text">
  <button data-action="click->hello#greet">Greet</button>
</div>

<%= render 'skills/shared/form', skill: @skill %>