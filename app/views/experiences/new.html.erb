<h1>Add Experiences</h1>



<%= form_with model: @experience do |f| %>
    <div>
        <%= f.label :title, "title" %>
        <%= f.text_field :title %>
    </div>

    <div>
        <%= f.label :begin_date, "begin_date" %>
        <%= f.date_field :begin_date %>
    </div>

    <div>
        <%= f.label :end_date, "end_date" %>
        <%= f.date_field :end_date %>
    </div>

    <div>
        <%= f.label :location, "location" %>
        <%= f.text_field :location %>
    </div>

    <div>
        <%= f.label :body, "description" %>
        <%= f.text_area :body %>
    </div>

    <div>
        <%= f.submit "Submit" %>
    </div>
<% end %>