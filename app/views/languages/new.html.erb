<h1>Add Languages</h1>

<%= form_with model: @language do |f| %>
    <div>
        <%= f.label :name, "name" %>
        <%= f.text_field :name %>
    </div>

    <div>
        <%= f.label :level, "level" %>
        <%= f.select :level, ["beginner", "intermediate","expert"], selected: nil %>
    </div>

    <div>
        <%= f.submit "Submit" %>
    </div>
<% end %>